<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Heating_Control_UI"
             x:Class="Heating_Control_UI.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
	<Application.Styles>
		<StyleInclude Source="/Views/ShellControl.axaml"/>
	</Application.Styles>

	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Accents/ApplicationResourceDictionary.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>

	</Application.Resources>


	<Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>



	<Application.Styles>
        <FluentTheme/>
    </Application.Styles>
</Application>